body {
	margin: 0;
	
	font-size: 13px;
	font-family: Arial, Helvetica, sans-serif;
}

#wrapper {
	width: 1024px;
	height: 672px;
	margin: 0 auto;
	position:relative;
	z-index:0;
	
	background: #ffffff;
	
	-webkit-box-shadow: 0px -10px 10px rgba(50, 50, 50, 0.75);
-moz-box-shadow:    0px -10px 10px rgba(50, 50, 50, 0.75);
box-shadow:         0px -10px 10px rgba(50, 50, 50, 0.75);
}

a {
	position:relative;
}
a:hover {
	text-decoration:none;
}
a.disabled {
	text-decoration:none;
}

h1, h2, h3, h4, h5, h6 {
    color: #AE1912;
    font-family: Trebuchet MS,Arial,Helvetica,sans-serif !important;
    text-align: left;
}
h1 {
    color: #AE1912;
    font-size: 18px;
}
h2 {
    color: #000;
    font-size: 16px;
}
h2 .highlighted {
	color: #AE1912;
}

.lt-ie9 .hidden.pie {
    display: inline-block !important;
    visibility: visible;
	margin-left:-9000px;
	right:auto !important;
}
.lt-ie8 .hidden.pie {
    display: block !important;
}

.off-screen {
	margin-left:-9000px;
}

/* ==========================================================================
   Top section
   ========================================================================== */
.site-title {
    color: #FFFFFF;
    font-size: 16px;
    font-weight: bold;
    position: absolute;
    right: 60px;
    text-align: right;
    top: 68px;
}

#navigation-container { /* header container */
	background:transparent url(images/bg_p1.png) top left no-repeat;
	font:normal 12px Arial, Helvetica, sans-serif;
	height:148px;
	margin-top:10px;
	position:relative;
}

/* MAIN MENUS */
.main-menu-container {
	position:absolute;
	left:0px;
	top:98px;
	font-family: Trebuchet MS,Arial,Helvetica,sans-serif !important;
    font-size: 1em !important;
    font-weight: bold;
	width: 100%;
    height: 34px;
    z-index: 20;
	padding: 4px 0 0 0;
}
/*first level*/
.main-menu-container ul {
	/*height: 35px;*/
	padding:0px;
	margin:0px;
	list-style:none;
}
.main-menu-container li {
	display: inline;
    float: left;
    list-style: none outside none;
    margin: 0;
    padding: 0;
    position: relative;
    text-align: left;
    z-index: 20;
}
.main-menu-container li:hover {
	background-image: url(images/nav_horizontal_glow.png);
    background-position: right center;
    background-repeat: no-repeat;
}
.main-menu-container li:hover a {
	color:#FFFFFF;
}
.main-menu-container li:hover ul.submenu a {
	color:#333333;
}
.main-menu-container li:hover ul.submenu a:hover {
	color:#FFFFFF;
}

.main-menu-container li a {
	background-color: transparent;
    border: medium none;
    color: #666666;
    display: block;
    float: none !important;
    font-family: Trebuchet MS,Arial,Helvetica,sans-serif !important;
    font-size: 1em !important;
    height: 32px !important;
    line-height: 32px;
    padding: 0 20px 0 7px;
    text-decoration: none;
    white-space: nowrap;
}
.main-menu-container li a:hover {
	color:#FFFFFF;
}

.main-menu-container ul li a.current {
	background-image: url(images/nav_horizontal_glow.png);
    background-position: right center;
    background-repeat: no-repeat;
    color: #FFFFFF;
}
/*second level*/
ul.submenu {
	display:none;
	width:auto; /* Width to help Opera out width: 190px;*/
	/*-moz-box-shadow: 4px 4px 5px #666;
	-webkit-box-shadow: 4px 4px 5px #666;
	box-shadow: 4px 4px 5px #666;*/
    position: absolute;
	left: 0;
    top: 32px;
    margin:0px;
	padding:0px;
	
	border-bottom: 1px solid #B6B6B6;
    border-left: 1px solid #B6B6B6;
    background: url(images/nav_horizontal_items_bg.gif) repeat-x scroll left top transparent;
    list-style: none outside none;
    
    z-index: 200;
	
	clear:both;
}

ul.submenu li {
	display: block !important;
    float: none;
    /*height: auto;*/
    width: 190px;
    z-index: 20;
}
ul.submenu li:hover {
	background:none;
}
ul.submenu a {
	color:#333333;
	background:none;
	padding:0px 15px;
	height:30px;
	line-height:30px;
	font-weight:bold;
}

#navigation-container-side {
	display:none;
}

/* SECONDARY MENU */
.sec-menu-container {
	position:absolute;
	top:5px;
	right:40px;
}
.sec-menu-container ul, .sec-menu-container li {
	padding:0px;
	margin:0px;
	list-style:none;
	display:block;
	float:left;
}
.sec-menu-container li {
	border-width: 0 1px 0 0;
	border-color: #cacaca;
	border-style:solid;
}
.sec-menu-container li:last-child, .sec-menu-container li.last {
	border-width: 0;
}
.sec-menu-container a {
	color:#0000CC;
	text-decoration:none;
	font:normal 12px Trebuchet MS,Arial,Helvetica,sans-serif !important;
	line-height:20px;
	padding:0px 5px;
	text-decoration:underline;
}
.sec-menu-container a:hover {
	color:#0000CC;
	text-decoration:none;
}

/* ==========================================================================
   Content overlay (mentor, titles etc.)
   ========================================================================== */
.overlay-content {
	z-index:100;
}  

/* ==========================================================================
   Content
   ========================================================================== */

.module-content {
    /*height: 536px;
    overflow: hidden;*/
    position: absolute;
    top: 136px;
    width: 1024px;
}

.content {
    /*height: 516px;
    overflow: auto;*/
    left: 0;
    padding: 10px 20px;
    margin-bottom: 20px;
    min-height: 580px;
    /*position: absolute;*/
    top: 0;
    /*width: 984px;*/
    z-index: 0;
    -webkit-box-shadow: 0px 2px 10px rgba(50, 50, 50, 0.75);
-moz-box-shadow:    0px 2px 10px rgba(50, 50, 50, 0.75);
box-shadow:         0px 2px 10px rgba(50, 50, 50, 0.75);
    
}

.screen-content {
	height:auto;
	width:100%;
}

.screen-content p {
	margin-bottom: 20px;
}
.screen-content p:last-child, .screen-content p.last {
    margin-bottom: 0;
}

.doc-wrapper {
	background-color: #fff;
    border: 1px solid #DDDDDD;
    margin: 0 40px;
    padding: 0 15px 15px;
	
	box-shadow: 0px 0px 5px #ccc; -webkit-box-shadow: 0px 0px 5px #ccc; -moz-box-shadow: 0px 0px 5px #ccc;
}

.full-width{
    width: 90%;
}

.full-width th {
	color: #FFFFFF;
    font-weight: bold;
}

.full-width tr.subheading td {
	background-color: #666666;
    color: #FFFFFF;
    font-weight: bold;
    padding: 5px 10px;
}

.full-width td {
	width: 33%;
	text-align:left;
}

.table-fix1 td{
	width: auto;
}

.tb-header td {
    background: none repeat scroll 0 0 #AE1912;
    color: #FFFFFF;
    font-weight: bold;
}
.td-color{
	 background: none repeat scroll 0 0 #AE1912;
    color: #FFFFFF;
    font-weight: bold;
}
.full-width td.thin {
	width:50px;
	text-align:center;
}

a.checkbox {
	background-color:#fff;
    border: 1px solid #999;
    display: block;
    height: 18px;
    margin: 0 auto;
    width: 18px;
	text-decoration:none;
}

.checkbox.checked:before {
    background: url("images/tick.png") no-repeat scroll 0 0 transparent;
    content: "";
    display: block;
    height: 25px;
    margin-top: -5px;
    width: 25px;
}

a.checkbox:hover, a.checkbox.ui-state-hover {
	background-color:#ddd;
}

.lt-ie8 a.checkbox.checked {
    background: url("images/tick.png") no-repeat scroll -4px -4px transparent;
}

.lt-ie8 a.checkbox.checked:hover {
    background-color:#ddd;
}

table.password {
	width: 290px;
	position:relative;
}
td.prompt.email input {
    float: left;
    position: relative;
    top: 2px;
}
.cbText {
    display: block;
    margin-left: 20px;
}

.label {
    padding-left: 20px;
    padding-right: 35px;
}

table.login {
	margin: 100px auto;
    width: 290px;
}
table.login td, table.password td {
    padding: 10px 20px 0;
    text-align: left;
}
table.login td.inputfield, table.password td.inputfield {
    padding-bottom: 8px;
    padding-top: 5px;
}
table.login td.inputfield input, table.password td.inputfield input {
    height: 22px;
    width: 100%;
}
table.login td.submit, table.password td.submit {
    padding-bottom: 10px;
    text-align: right;
}
table.login td.submit input, table.password td.submit input {
    margin-right:0;
}

table {
    background-color: #CACACA;
    border-collapse:separate;
	border-spacing:1px;
	margin: 0 40px 20px;
}

table.nogrid {
	border-collapse:collapse;
	border-spacing:0;
	border:1px solid #CACACA;
}

th.left {
    padding: 10px 20px;
    text-align: left;
}

td, th {
	background-color:#efefef;
	padding:3px 10px;
	text-align:center;
}

th {
	padding:10px 10px;
	color:#fff;
	background-color:#666;
}

th a, th a:active {
	color:#fff;
	padding: 0 10px;
}

th a.active {
    background: url("images/sort-arrow.png") no-repeat scroll 0 5px transparent;
	padding: 0 5px 0 15px;
}

th.name-title {
	width:150px;
}

td.name {
	text-align:left;
}

td.checklist, td.assessment {
    font-size: 16px;
}

table.password .password-display td, table.login .password-display td {
    border-top: 1px solid #CACACA;
	background-color:#666666;
	color:#fff;
	padding-bottom:10px;
}

.highlight {
	font-weight:bold;	
}

p.box-title {
    background-color: #666666;
    color: #FFFFFF;
    font-weight: bold;
	padding: 10px 20px;
}

.box-content {
	padding: 0 20px 10px;
}

.filters {
    background-color: #EFEFEF;
    border: 1px solid #CACACA;
    float: right;
    max-width: 450px;
	margin: 0 40px;
}

.filter > p {
    display: inline-table;
    font-weight: bold;
    margin-right: 10px;
    text-align: right;
    width: 100px;
	margin-bottom: 0;
}

input[type="submit"], input[type="reset"], input[type="button"], .button, .standard-button, .ui-dialog .ui-button {
	display: inline-block;
	text-decoration: none;
	text-align:center;
	
	position: relative;
	cursor: pointer;
	font-size: 11px;
	font-weight: bold;
	line-height: 1;
	margin-right:3px;
	
	
	background: url(images/button_bg.gif) repeat-x scroll left top #BD4841;
    border-color: #70110C;
    border-left: 1px solid #70110C;
    border-style: solid;
    border-width: 1px;
	border-radius:0;
    color: #FFFFFF;
    font-family: Trebuchet MS,Arial,Helvetica,sans-serif;
    font-size: 12px;
    font-weight: bold;
    padding: 5px 8px;
	
	min-width:60px;
}

input[type="submit"]:hover, input[type="reset"]:hover, input[type="button"]:hover, .button:hover, .standard-button:hover, .standard-button.ui-state-hover {
	text-decoration:underline;
}

input[type="submit"].disabled, input[type="reset"].disabled, input[type="button"].disabled, .button.disabled, .standard-button.disabled {
	background: none repeat scroll 0 0 #D5CDCD;
    border-color: #999999;
    color: #666666;
    text-decoration: none;
}

.back-button {
    position: absolute;
    right: 0;
    top: -30px;
}

select {
    margin-bottom: 20px;
}

table.dataTable tfoot select {
    margin-bottom: 5px;
}

.button-pane {
	text-align:right;
}

.standard-button.apply {
    
}

.separator {
	margin-right: 30px;
}


/* ==========================================================================
   Click/reveal tooltips
   ========================================================================== */

.ui-tooltip.guide {
    left: 280px;
    position: absolute;
    top: 10px;
    width: 270px;
}
   
.ui-tooltip {
	padding: 8px 20px;
	color:#fff;
	background-color: #656565;
	position: relative;
	border: 2px solid #fff;
	border-radius:20px;
	max-width: none;
	
	/*box-shadow: 0px 0px 5px #000000; -webkit-box-shadow: 0px 0px 5px #000000; -moz-box-shadow: 0px 0px 5px #000000;*/
}

.ui-tooltip p {
	margin:0;
	padding:0;
}

.ui-tooltip:after, .ui-tooltip:before {
	border: solid transparent;
	content: ' ';
	height: 0;
	position: absolute;
	width: 0;
}
.ui-tooltip:after {
	border-width: 12px;
}
.ui-tooltip:before {
	border-width: 16px;
}

.ui-tooltip.bottom:after {
	border-top-color: #656565;
	top:100%;
	left: 50%;
	margin-left:-12px;
}
.ui-tooltip.bottom:before {
	border-top-color: #fff;
	top:100%;
	left: 50%;
	margin-left:-16px;
}
.ui-tooltip.bottom-left:after {
	border-top-color: #656565;
	top:100%;
	left: 24px;
}
.ui-tooltip.bottom-left:before {
	border-top-color: #fff;
	top:100%;
	left: 20px;
}
.ui-tooltip.bottom-right:after {
	border-top-color: #656565;
	top:100%;
	right: 24px;
}
.ui-tooltip.bottom-right:before {
	border-top-color: #fff;
	top:100%;
	right: 20px;
}

.ui-tooltip.top:after {
	border-bottom-color: #656565;
	bottom:100%;
	left: 50%;
	margin-left:-12px;
}
.ui-tooltip.top:before {
	border-bottom-color: #fff;
	bottom:100%;
	left: 50%;
	margin-left:-16px;
}
.ui-tooltip.top-left:after {
	border-bottom-color: #656565;
	bottom:100%;
	left: 24px;
}
.ui-tooltip.top-left:before {
	border-bottom-color: #fff;
	bottom:100%;
	left: 20px;
}
.ui-tooltip.top-right:after {
	border-bottom-color: #656565;
	bottom:100%;
	right: 24px;
}
.ui-tooltip.top-right:before {
	border-bottom-color: #fff;
	bottom:100%;
	right: 20px;
}

.ui-tooltip.left:after {
	border-right-color: #656565;
	right:100%;
	top: 50%;
	margin-top:-12px;
}
.ui-tooltip.left:before {
	border-right-color: #fff;
	right:100%;
	top: 50%;
	margin-top:-16px;
}
.ui-tooltip.left-top:after {
	border-right-color: #656565;
	right:100%;
	top: 24px;
}
.ui-tooltip.left-top:before {
	border-right-color: #fff;
	right:100%;
	top: 20px;
}
.ui-tooltip.left-bottom:after {
	border-right-color: #656565;
	right:100%;
	bottom: 24px;
}
.ui-tooltip.left-bottom:before {
	border-right-color: #fff;
	right:100%;
	bottom: 20px;
}

.ui-tooltip.right:after {
	border-left-color: #656565;
	left:100%;
	top: 50%;
	margin-top:-12px;
}
.ui-tooltip.right:before {
	border-left-color: #fff;
	left:100%;
	top: 50%;
	margin-top:-16px;
}
.ui-tooltip.right-top:after {
	border-left-color: #656565;
	left:100%;
	top: 24px;
}
.ui-tooltip.right-top:before {
	border-left-color: #fff;
	left:100%;
	top: 20px;
}
.ui-tooltip.right-bottom:after {
	border-left-color: #656565;
	left:100%;
	bottom: 24px;
}
.ui-tooltip.right-bottom:before {
	border-left-color: #fff;
	left:100%;
	bottom: 20px;
}

/* some fallback for IE7 */
.lt-ie8 .ui-tooltip.top-left, .lt-ie8 .ui-tooltip.left-top {
	border-radius:0 20px 20px;
}
.lt-ie8 .ui-tooltip.top {
	border-radius:0  0 20px 20px;
}
.lt-ie8 .ui-tooltip.top-right, .lt-ie8 .ui-tooltip.right-top {
	border-radius:20px 0 20px 20px;
}
.lt-ie8 .ui-tooltip.right {
	border-radius:20px  0 0 20px;
}
.lt-ie8 .ui-tooltip.bottom-right, .lt-ie8 .ui-tooltip.right-bottom {
	border-radius:20px 20px 0 20px;
}
.lt-ie8 .ui-tooltip.bottom {
	border-radius:20px 20px 0 0;
}
.lt-ie8 .ui-tooltip.bottom-left, .lt-ie8 .ui-tooltip.left-bottom {
	border-radius:20px 20px 20px 0;
}
.lt-ie8 .ui-tooltip.left {
	border-radius:0 20px 20px 0;
}

.tick {
    background: url("images/tick.png") no-repeat scroll 0 0 transparent;
    display: block;
    height: 25px;
    margin: 0 auto;
    position: relative;
    width: 25px;
}

/* ==========================================================================
   Generic styles
   ========================================================================== */
   
.debug {
    position: absolute;
	right: 420px;
    top: 60px;
}

.wrapper p.dev-notes {
    position: absolute;
    left: 500px;
    top: 10px;
    width: 490px;
    background-color: #fff;
    border: 1px dotted #999;
    color: #0000FF;
	font-size: 0.9em;
	font-style:italic;
    padding: 0 5px;
    z-index: 20;
	
	opacity: 0.75;
	filter: Alpha(Opacity=75);
	
	/*display:none;*/
}

.standard-button .disabled {
	text-decoration: none;
}

.shadow{
	box-shadow: 0 0 5px #000000;
}
.no-pointer{
	cursor: default;
}

/* ==========================================================================
   Dialogs
   ========================================================================== */
   
.dialog {
	display:none;
}

.ui-button-text-only .ui-button-text {
	padding:0;
}
   
.ui-widget {
	font-family: Arial,Helvetica,sans-serif;
	font-size:13px;
}

.ui-widget-overlay {
    background: none repeat-x scroll 50% 50% #666666;
    opacity: 0.8;
	filter: Alpha(Opacity=80);
}
.ui-widget input, .ui-widget select, .ui-widget textarea, .ui-widget button {
	font-family: Calibri, "Helvetica Neue", Helvetica, Arial, sans-serif;
}

.ui-dialog .ui-dialog-title {
	font-size:16px;
}

.ui-state-hover .ui-icon {
	background-image: url(redmond/images/ui-icons_cd0a0a_256x240.png);
}

.ui-dialog-titlebar-close.ui-state-hover {
	background-image:none;
	background-color:#ecd9d9;
	border-color:#70110C;
}

.ui-dialog {
	padding:2px;
	border-radius: 5px;
	background:none repeat scroll 0 0 #eaeaea;
	position: absolute;
	max-width: none;
}
.ui-dialog .ui-dialog-titlebar {
    background: none repeat scroll 0 0 #656565;
    border: medium none;
}
.ui-dialog .ui-dialog-titlebar-close {
	margin: -10px 5px 0 0;
}
.ui-dialog .ui-dialog-buttonpane {
    background: none;
	border-color: #bbb;
	padding: 0.2em 1em;
}
.ui-dialog .ui-dialog-buttonpane button {
	margin: 0.5em 0.4em 0.5em 0.5em;
}

.ui-dialog p {
	margin: 5px 0 15px;
}

.ui-dialog-content {
	max-height: 520px;
}